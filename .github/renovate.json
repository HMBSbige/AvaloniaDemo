{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "HMBSbige"
  ],
  "dependencyDashboard": false,
  "extends": [
    "config:recommended",
    ":configMigration",
    ":automergeBranch",
    ":automergeDigest",
    ":automergeMinor",
    ":disableRateLimiting"
  ],
  "packageRules": [
    {
      "matchSourceUrls": [
        "https://github.com/abpframework/abp"
      ],
      "groupName": "abp"
    },
    {
      "groupName": "ReactiveUI",
      "matchSourceUrls": [
        "https://github.com/reactiveui/{/,}**"
      ]
    }
  ],
  "labels": [
    "Automatic"
  ]
}
